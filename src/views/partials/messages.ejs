<% if(typeof errors != "undefined") { %>
    <% errors.forEach(function(error) { %> 
        <div role="alert alert-warning alert-dismissible fade show" role="alert" id="message">
            <%= error.msg %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close" id="btn-close">X</button> 
        </div> 
    <% }) %> 
<% } %>